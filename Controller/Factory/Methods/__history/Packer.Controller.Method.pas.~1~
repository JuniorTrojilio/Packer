unit Packer.Controller.Method;

interface

uses
  Packer.Controller.Method.Interf, Packer.Controller.Method.Zip.Interf;
type
    TPackerMethods = class(TInterfacedObject, iPackerMethods)
    private
      { Private declarations }
    public
      { Public declarations }
      function Zip : iZip;
      constructor Create;
      destructor Destroy; override;
      class function New : iPackerMethods;
    end;

implementation

{ TPackerMethod }

uses Packer.Controller.Method.Zip;

constructor TPackerMethods.Create;
begin

end;

destructor TPackerMethods.Destroy;
begin

  inherited;
end;

class function TPackerMethods.New: iPackerMethods;
begin
  Result := Self.Create;
end;

function TPackerMethods.Zip: iZip;
begin
  Result :=  TZip.New;
end;

end.
